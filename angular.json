{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-19-material-theming": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/angular-19-material-theming",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              "src/favicon.ico",
              {
                "glob": "favicon-48x48.png",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "apple-touch-icon.png",
                "input": "src",
                "output": "/"
              },

              {
                "glob": "Logo.png",
                "input": "src",
                "output": "/"
              },

              "src/assets",
              "src/robots.txt",
              "src/sitemap.xml",
              "src/google61fae8466f3cfebd.html"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/rose-red.css",
              "node_modules/leaflet/dist/leaflet.css",
              "src/styles.scss",
              "node_modules/aos/dist/aos.css"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/styles"
              ]
            },
            "server": "src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "src/server.ts"
            },
            "prerender": false
          },
          "configurations": {
            "production": {
              "prerender": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kB",
                  "maximumError": "20kB"
                }
              ],
              "outputHashing": "all",
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/app/styles"
                ]
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "angular-19-material-theming:build:production"
            },
            "development": {
              "buildTarget": "angular-19-material-theming:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              "src/favicon.ico",
              "src/assets",
              "src/sitemap.xml",
              "src/robots.txt",
              "src/google61fae8466f3cfebd.html"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/rose-red.css",
              "src/styles.scss",
              "node_modules/aos/dist/aos.css"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/styles"
              ]
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "41a7d780-086a-4c37-86cf-c84d6931f92a"
  }
}
